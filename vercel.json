{
    "github": {
        "silent": true
    },
    "functions": {
        "api/chewbacca/generateSummaryOfQualifications/route.js": {
            "maxDuration": 60
        }
    },
    "headers": [
        {
            "source": "/(.*)",
            "headers": [
                {
                    "key": "X-Real-IP",
                    "value": "$remote_addr"
                },
                {
                    "key": "X-Forwarded-For",
                    "value": "$proxy_add_x_forwarded_for"
                },
                {
                    "key": "Host",
                    "value": "$http_host"
                }
            ]
        }
    ]
}
